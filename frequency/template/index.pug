        .col-12.col-md-6.gx-sm-0
          img(src="./img/banner-watch-women.jpg" alt="banner")

      .row
        - var items = [{img: "./img/watch-w1.jpg", style: "HAMILTON - 32MM", name: "ANNE", price: "9.299.000 ₫"},{img: "./img/watch-w2.jpg", style: "HAMILTON - 32MM", name: "CORA", price: "12.209.000 ₫"},{img: "./img/watch-w3.jpg", style: "MORAINE - 28MM", name: "LINGER", price: "8.099.000 ₫"},{img: "./img/watch-w4.jpg", style: "HAMILTON - 32MM", name: "SYLVIA", price: "9.199.000 ₫"}]

